<IfModule mod_rewrite.c>
    RewriteEngine On

    # Sesuaikan base path dengan folder project kamu
    RewriteBase /api-php-native/public/

    # Jika file atau folder fisik tidak ada, arahkan ke index.php
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^ index.php [L]
</IfModule>
